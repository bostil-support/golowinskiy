@using Golowinskiy_NewBostil.Entities
@model Dictionary<User, int> 

@{
    ViewData["Title"] = "AdminPanel";
}

<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link rel="stylesheet" href="~/css/admin.css" />

<div class="showSpinner">
    <i class="fa fa-spinner fa-spin"></i>
</div>
<div class="dropdownUL">
<h4>Список пользователей</h4>
<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item" onclick="GetUserProducts('@item.Key.Id')"><i class="fa fa-user" aria-hidden="true"></i>@item.Key.UserName (@item.Value)</li>
    }
</ul>
    </div>
<script src="~/js/admin.js"></script>
